{
  "name": "react-redux-rxjs",
  "version": "0.1.0",
  "dependencies": {
    "autodll-webpack-plugin": "^0.3.5",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "bootstrap-sass": "^3.3.7",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "^0.59.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "20.0.4",
    "jest-fetch-mock": "^1.3.3",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "postcss-scss": "^1.0.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.1.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-transition-group": "^2.2.1",
    "sass-loader": "^6.0.6",
    "serve": "^6.4.1",
    "style-loader": "0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-scss": "^2.1.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "serve": "serve -s build -p 3000",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/narr/react-redux-rxjs.git"
  },
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          },
          "generateScopedName": "[local]___[hash:base64:5]"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-sass-guidelines"
    ],
    "rules": {
      "selector-type-no-unknown": [
        true,
        {
          "ignore": [
            "custom-elements"
          ]
        }
      ],
      "selector-class-pattern": "^[a-zA-Z0-9\\-]+$",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  }
}
